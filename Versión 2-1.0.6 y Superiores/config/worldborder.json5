	// min: -100000, max: 0
	"netherBorderNegativeX": -625,
	// El borde del Nether ubicado en la coordenada z positiva.
	// min: 0, max: 100000
	"netherBorderPositiveZ": 625,
	// El borde del Nether ubicado en la coordenada z negativa.
	// min: -100000, max: 0
	"netherBorderNegativeZ": -625,
	// El borde del End ubicado en la coordenada x positiva.
	// min: 0, max: 100000
	"endBorderPositiveX": 5000,
	// El borde del End ubicado en la coordenada x negativa.
	// min: -100000, max: 0
	"endBorderNegativeX": -5000,
