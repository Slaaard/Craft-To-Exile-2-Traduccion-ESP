	// La cantidad de bloques que el jugador es teletransportado hacia adentro después de golpear el borde.
	// min: 0, max: 1000
	"distanceTeleportedBack": 10,
	// El mensaje que se enviará al jugador cuando esté dentro de 'distanceTeleportedBack' del borde del mundo.
	"nearBorderMessage": "¡Te estás acercando al borde del mundo!",
	// El mensaje que se enviará al jugador cuando golpee el borde del mundo.
	"hitBorderMessage": "¡Has golpeado el borde del mundo y has sido teletransportado hacia adentro!",
	// El mensaje enviado al jugador cuando golpea el borde y 'shouldLoopToOppositeBorder' está activado.
	"loopBorderMessage": "¡Has golpeado el borde del mundo y has dado la vuelta al mundo!",
	// El borde del Overworld ubicado en la coordenada x positiva.
	// min: 0, max: 100000
	"overworldBorderPositiveX": 50000,
	// El borde del Overworld ubicado en la coordenada x negativa.
	// min: -100000, max: 0
	"overworldBorderNegativeX": -50000,
	// El borde del Overworld ubicado en la coordenada z positiva.
	// min: 0, max: 100000
	"overworldBorderPositiveZ": 50000,
	// El borde del Overworld ubicado en la coordenada z negativa.
	// min: -100000, max: 0
	"overworldBorderNegativeZ": -50000,
